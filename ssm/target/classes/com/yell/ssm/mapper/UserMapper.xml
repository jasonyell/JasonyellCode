<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yell.ssm.dao.UserDao">
    <select id="queryUserByUserId" parameterType="int" resultType="com.yell.ssm.po.User">
        select user_id userID,user_name userName,user_pwd userPwd from tb_user where user_Id = #{userId}
    </select>
</mapper>
